{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "grain-lang",
  "title": "Grain",
  "description": "This extension allows to search through the grain lang documentation from Raycast.",
  "icon": "Grain.png",
  "author": "spotandjake",
  "contributors": [
    "spotandjake"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Grain",
      "subtitle": "search",
      "description": "Search through an Algolia index directly from Raycast.",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.26.1",
    "algoliasearch": "^5.1.3",
    "node-fetch": "^3.3.2",
    "prettier": "^2.5.0"
  },
  "devDependencies": {
    "@types/node": "~16.10.0",
    "@types/react": "^17.0.28",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "react-devtools": "^4.19.2",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop"
  }
}
